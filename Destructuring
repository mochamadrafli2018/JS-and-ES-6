//Destructuring
// array
var numbers = [1,2,3]
var numberOne = numbers[0]
var numberTwo = numbers[1]
var numberThree = numbers[2]
console.log(numberOne)
// object
var studentName = {
    afirstName: 'Peter',
    alastName: 'Parker'
};
const afirstName = studentName.afirstName;
const alastName = studentName.alastName;
console.log(afirstName)

//Dengan destructuring:
// array
let bnumbers = [1,2,3]
const [bnumberOne,bnumberTwo,bnumberThree] = bnumbers
console.log(bnumberOne)
// object
var studentName = {
    bfirstName: 'Peter',
    blastName: 'Parker'
};
const {bfirstName, blastName} = studentName
console.log(bfirstName)

/*Kesimpulan
1. Destructuring untuk membuat variabel baru dari parameter object
*/
